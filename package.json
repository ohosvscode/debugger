{
  "name": "@arkts/debugger",
  "type": "module",
  "version": "0.0.1",
  "description": "Debugger for ArkTS",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./ws": {
      "types": {
        "import": "./dist/ws.d.mts",
        "require": "./dist/ws.d.cts"
      },
      "import": "./dist/ws.mjs",
      "require": "./dist/ws.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "scripts": {
    "build": "tsdown --format esm,cjs src/index.ts src/ws.ts",
    "test": "vitest --coverage --ui --globals"
  },
  "dependencies": {
    "@types/ws": "^8.18.1",
    "mitt": "^3.0.1",
    "vitest": "^4.0.17",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@vitest/coverage-v8": "4.0.17",
    "@vitest/ui": "4.0.17",
    "eslint": "^9.39.2",
    "tsdown": "^0.20.0-beta.4",
    "typescript": "^5.9.3"
  }
}
